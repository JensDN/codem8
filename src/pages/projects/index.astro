---
import StarlightPage, {
  type StarlightPageProps,
} from "@astrojs/starlight/components/StarlightPage.astro";
import { ProjectsIndex } from "@/components/react/projects";

const PROJECTS_PAGE_PROPS: StarlightPageProps = {
  frontmatter: {
    title: "Projects",
    tableOfContents:false
  },
  hasSidebar: false,
};
---

<StarlightPage
  frontmatter={PROJECTS_PAGE_PROPS.frontmatter}
  hasSidebar={PROJECTS_PAGE_PROPS.hasSidebar}
>
<ProjectsIndex client:load></ProjectsIndex>
</StarlightPage>
